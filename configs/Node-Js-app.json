{
  "directories": {
    "src": {
      "baseDir": "src",
      "subDir": [
        "config",
        "controllers",
        "services",
        "models"
      ]
    },
    "scripts": {
      "baseDir": "scripts"
    }
  },
  "commands": {
    "tsc-init": {
      "base": "tsc",
      "arguments": [
        "--init"
      ]
    },
    "npm-init": {
      "base": "npm",
      "arguments": [
        "init",
        "-y"
      ]
    }
  },
  "files": {
    "eslintr": {
      "name": ".eslintrc",
      "directory": "./"
    }
  }
}
